#Assignment2 Exercise 1
library(tidyverse)
#display the car with over 100 and below 200 horsepower
mtcars_hp_high <- function(data){
mtcars_1 <- data %>%
filter(hp>100) %>% # over 100 horsepower
filter(hp<200) %>% # below 200 horsepower
arrange(hp)
return(mtcars_1)
}
mtcars_hp_high(mtcars)
#Assignment2 Exercise 1
library(tidyverse)
#display the car with over 100 and below 200 horsepower and arrange the order by horsepower
mtcars_hp_high <- function(data){
mtcars_1 <- data %>%
filter(hp>100) %>% #over 100 horsepower
filter(hp<200) %>% #below 200 horsepower
arrange(hp) #arrange the filtered data by horsepower
return(mtcars_1)
}
mtcars_hp_high(mtcars)
